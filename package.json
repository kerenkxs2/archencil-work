  "main": "app/main.js",
  "private": true,
  "electronProjectPath": "Z:/development/archencil",
  "scripts": {
    "postinstall": "install-app-deps",
    "ng:serve": "ng serve -c web -o",
    "build": "ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "serve": "ng serve -c web -o",
    "web": "fuser -k 4200/tcp || true && ng serve -c web",
    "build:t": "tsc",
    "build:prod": "npm run build -- -c production",
    "web:build": "npm run build -- -c web-production",
    "local": "npm run local:wsl",
    "start-server": "npx http-server ./dist/project -p 8080",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint_ignored": "eslint -c .eslintrc.js --ext .ts /home/matheus/development/archencil/",
    "lint_original": "ng lint",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html}' --write",
    "lint:styles": "stylelint '**/*.scss' --config .stylelintrc.json",
    "prelocal": "npm run build && stylelint '**/*.scss' --config .stylelintrc.json && npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet",
    "preweb": "npm run build && stylelint '**/*.scss' --config .stylelintrc.json && npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet && npm-run-all -p",
    "ng:serve:unix": "ng serve -c web & wait",
    "clean-reset": "rm -rf node_modules && npm cache clean --force && npm install"
  },
